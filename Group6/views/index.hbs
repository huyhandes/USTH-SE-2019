  <header class="masthead" style="background-image: url('img/home-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
            <div class="site-heading">
            <h1>Movie Blog</h1>
            </div>
        </div>
        </div>
    </div>
</header>
<div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-10 mx-auto">
        {{# each items}}
        <div class="post-preview" id="{{this.id}}">
          <a href="/post/{{this.id}}">
            <h2 class="post-title">
              {{this.title}}
            </h2>
            <h3 class="post-subtitle">
              {{this.subtitle}}
            </h3>
          </a>
          <p class="post-meta">Posted by admin on {{this.postedtime}}</p>
        </div>
        {{/each}}
        <hr>
        <!-- Pager -->
        <div class="clearfix">
          <a class="btn btn-primary float-right" href="#">Older Posts &rarr;</a>
        </div>
      </div>
    </div>
</div>