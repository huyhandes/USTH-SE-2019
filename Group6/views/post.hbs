<header class="masthead" style="background-image: url('img/post-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          {{# each items}}
          <div class="post-heading">
            <h1>{{this.headerName}}</h1>
            <h2 class="subheading">{{this.ummary}}</h2>
            <span class="meta">Posted by {{this.user}} on {{this.date}}</span>
          </div>
          {{/each}}
        </div>
      </div>
    </div>
  </header>

  <!-- Post Content -->
  <article>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <pre>

          </pre>
        </div>
      </div>
    </div>
  </article>